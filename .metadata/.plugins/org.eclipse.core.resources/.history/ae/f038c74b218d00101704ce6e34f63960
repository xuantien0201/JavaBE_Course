
public class DemoOOP {
	public static void main(String[] args) {
		sinhVien sinhVienA = new sinhVien("Nguyen Van A", "12A1", 30, "19-02-2000");
		sinhVien sinhVienB = new sinhVien();
		sinhVienB.setHoTen("Nguyen Van B");
		
		giaoVien giaoVienA = new giaoVien();
		giaoVienA.setHoTen("Nguyen Thi Ha");
		System.out.println("Kiem Tra: " + sinhVienA.hoTen);
		System.out.println("Kiem Tra: "+ sinhVienA.lop);
		System.out.println("Kiem Tra: " + sinhVienB.hoTen);
		System.out.println("Kiem Tra GV: " + giaoVienA.hoTen);
	}
}
class giaoVien extends sinhVien {
	String monHoc;
}
class sinhVien {
	String hoTen;
	String lop;
	int tuoi;
	String ngaySinh;
	
	// phuong thuc khoi tao constructor co tham so truyen vao
	public sinhVien(String hoTen, String lop, int tuoi,String ngaysinh) {
		this.hoTen = hoTen;
		this.lop = lop;
		this.tuoi = tuoi;
		this.ngaySinh = ngaysinh;
	}
	// phuong thuc khoi tao constructor khong co tham so truyen vao
	public sinhVien() {
		
	}

	public String getHoTen() {
		return hoTen;
	}

	public void setHoTen(String hoTen) {
		this.hoTen = hoTen;
	}

	public String getLop() {
		return lop;
	}

	public void setLop(String lop) {
		this.lop = lop;
	}

	public int getTuoi() {
		return tuoi;
	}

	public void setTuoi(int tuoi) {
		this.tuoi = tuoi;
	}

	public String getNgaySinh() {
		return ngaySinh;
	}

	public void setNgaySinh(String ngaySinh) {
		this.ngaySinh = ngaySinh;
	}
}
