
public class DemoOOP {
	public static void main(String[] args) {
		sinhVien sinhVienA = new sinhVien("Nguyen Van A", "12A1", 30, "19-02-2000");
		System.out.println("sinh vien thu nhat: " + sinhVienA.hoTen + sinhVienA.tuoi);
	}
}
class sinhVien {
	String hoTen;
	String lop;
	int tuoi;
	String ngaySinh;
	
	public sinhVien(String hoTen, String lop, int tuoi,String ngaysinh) {
		this.hoTen = hoTen;
		this.lop = lop;
		this.tuoi = tuoi;
		this.ngaySinh = ngaysinh;
	}
}
